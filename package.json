{
  "name": "sokoban",
  "version": "1.0.0",
  "description": "A template for building CLI applications with Bun",
  "main": "index.ts",
  "bin": {
    "mycli": "index.ts"
  },
  "type": "module",
  "scripts": {
    "dev": "bun run --watch src/index.tsx",
    "start": "bun run src/index.tsx",
    "lint": "eslint .",
    "test": "vitest",
    "format": "prettier --write .",
    "format:check": "prettier --check ."
  },
  "keywords": [
    "cli",
    "template",
    "bun"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@eslint/js": "^9.36.0",
    "@types/react": "^19.2.2",
    "eslint": "^9.36.0",
    "eslint-config-prettier": "^10.1.8",
    "prettier": "^3.6.2"
  },
  "dependencies": {
    "@types/node": "^24.9.2",
    "fs": "^0.0.1-security",
    "ink": "^6.4.0",
    "react": "^19.2.0",
    "vitest": "^4.0.6"
  }
}
